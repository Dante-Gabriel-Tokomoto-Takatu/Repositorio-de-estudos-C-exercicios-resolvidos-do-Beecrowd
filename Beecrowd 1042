#include <stdio.h>

int main(){

    int a, b, c, aux1, aux2, aux3, comparador;
    scanf("%d %d %d", &a, &b, &c);
    aux1 = a;
    aux2 = b;
    aux3 = c;

    if (aux1 < aux2){

        comparador = aux1;
        aux1 = aux2;
        aux2 = comparador;
    }

    if (aux2 < aux3){

        comparador = aux2;
        aux2 = aux3;
        aux3 = comparador;
    }

    if (aux1 < aux2){

        comparador = aux1;
        aux1 = aux2;
        aux2 = comparador;
    }

    printf("%d\n%d\n%d\n\n%d\n%d\n%d\n",aux3,aux2,aux1,a,b,c);

    return 0;

}
