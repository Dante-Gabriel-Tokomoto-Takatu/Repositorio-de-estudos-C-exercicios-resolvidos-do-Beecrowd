#include <stdio.h>
int main (){
    double A, B, C, area, area_c, area_t, area_q, area_r;
    scanf ("%lf %lf %lf", &A, &B, &C);
    area = (A * C)/ 2;
    area_c = 3.14159 * (C * C);
    area_t = (A + B) * C/ 2;
    area_q = B * B;
    area_r = A * B;
    printf("TRIANGULO: %.3lf\n", area);
    printf("CIRCULO: %.3lf\n", area_c);
    printf("TRAPEZIO: %.3lf\n", area_t);
    printf("QUADRADO: %.3lf\n", area_q);
    printf("RETANGULO: %.3lf\n", area_r);
    return 0;
}
