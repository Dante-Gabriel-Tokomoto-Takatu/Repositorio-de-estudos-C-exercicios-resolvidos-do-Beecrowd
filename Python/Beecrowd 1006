def CalculaMedia(A, B, C):
    MEDIA = (A *  2.0 + B *  3.0 + C *  5.0) / (2.0 +  3.0 +  5.0)
    return MEDIA

def main():
    A = float(input())
    B = float(input())
    C = float(input())
    
    MEDIA = CalculaMedia(A, B, C)
    
    print(f"MEDIA = {MEDIA:.1f}")

if __name__ == "__main__":
    main()
